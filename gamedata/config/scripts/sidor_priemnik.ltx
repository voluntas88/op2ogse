; -*- mode: conf-windows; coding: windows-1251-dos -*-
[logic]
active = ph_sound1

[ph_sound1]
snd = radio_music_sidor
on_info = {+sidor_music} ph_sound1
on_info2 = {+sidor_tush} ph_sound2
tips = Выключить...
nonscript_usable = false
on_use = %=play_snd_from_obj2(device\click10b) =dsh_set_ogse_var_true(sidor_priemnik.quiet)% ph_sound0

[ph_sound2]
snd = sidor_tush
on_info = {+sidor_music} ph_sound1
on_info2 = {+sidor_tush} ph_sound2
tips = Выключить...
nonscript_usable = false
on_use = %=play_snd_from_obj2(device\click10b) =dsh_set_ogse_var_true(sidor_priemnik.quiet)% ph_sound0

[ph_sound0]
tips = Включить...
nonscript_usable = false
on_use = %=play_snd_from_obj2(device\click10b) =dsh_ogse_var_delete(sidor_priemnik.quiet)% ph_sound1
